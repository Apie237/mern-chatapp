{
    "rewrites": [{ "source": "/api/:path*", "destination": "/backend/src/index.js" }],
    "redirects": [{ "source": "/old-path", "destination": "/new-path", "permanent": true }],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
      }
    ]
  }